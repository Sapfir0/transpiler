{
    "<больше>":       {">" : [">"]}, 
    "<меньше>":       {"<" : ["<"]}, 
    "<равно>":        {"==" : ["=="]}, 
    "<больше-равно>": {">=" : [">="]}, 
    "<меньше-равно>": {"<=" : ["<="]}, 
    "<не равно>":     {"!=" : ["!="]},
    "<прибавить и происвоить>": {"+=" : ["+="]}, 
    "<неравно>":      {"/=" : ["/="]}, 
    "<умножить>":     {"*" : ["*"]}, 
    "<делить>":       {"/" : ["/"]}, 
    "<двоеточие>":       {":" : [":"]},
    "<присвоить>":    {"=" : ["="]}, 
    "<плюс>":         {"+" : ["+"]}, 
    "<минус>":        {"-" : ["-"]}, 
    "<and>":          {"and" : ["and"]}, 
    "<or>":           {"or" : ["or"]},
    "<откр. скобка>": {"(" : ["("]},
    "<закр. скобка>": {")" : [")"]},
    "<запятая>":      {"," : [","]},
    

    "<value>" : {
        "id" : ["<value_id>"],
        "num" : ["<value_num>"]
    },
    "<value_num>" : {
        "num" : ["num"],
        
        "<<put_in_tree>>" : true
    },
    "<value_id>" : {
        "id" : ["id"],
        
        "<<put_in_tree>>" : true
    }, 

    "<блок объявления переменных>":{
        "local" : ["local", "<объявление переменных>"]
    },
    "<объявление переменных>": {
        "id" : ["<модификатор доступа>", "<блок_id>", ":", "<тип>", ";", "<обяъвление переменных с запятой>"],
        "<<dont_push_if_empty>>" : true
    },
    "<объявление переменных с запятой>" : {
        "id" : ["<объявление переменных>"],
        "do" : [],
        "<<dont_push_if_empty>>" : true
    },
    "<блок_id>": {
        "id" : ["<value_id>", "<блок_id>"],
        "," : ["<запятая>", "<value_id>", "<блок_id>"],
        ":" : [],
        ")" : [],
        "<<dont_push_if_empty>>" : true
    },
    "<тип>": {
        "int" : ["int"],
        "string" : ["string"]
    },
    "<блоки функций>": {
        "{": [],
        "id" : ["<блок функции>", "<блоки функций>"],
        "}" : [],
        
        "<<dont_recursion_reversal>>" : true
    },
    "<блок функции>": {
        "def" : ["def", "<value_id>", "<аргументы функции>", "<тело функции>"]
    },
    "<тело функции>" : {
        "{" : ["{", "<тело>", "}"]
    },
    "<аргументы функции>": {
        "(" : ["(", "<аргумент функции>", "<следующие аргументы функции>", ")"]
    },
    "<аргумент функции>" : {
        "id" : ["<блок_id>", ":", "<тип>"],
        ")" : []
    },
    "<следующие аргументы функции>" : {
        "," : ["<запятая>", "<аргумент функции>", "<следующие аргументы функции>"],
        ")" : [],
        "<<dont_push_if_empty>>" : true
    },
    "<программа>": {
        "id": ["<тело>"],
        "num": ["<тело>"],
        "if": ["<тело>"],
        "def": ["<тело>"],
        ";": ["<тело>"],
        "for": ["<тело>"],
        "mutable" : ["<тело>"],
        "}": [],
        "else": [],
        "return" : ["<тело>"]
    },
    "<тело>": {
        "id": ["<действие>", "<тело>"],
        "num": ["<действие>", "<тело>"],
        "if": ["<блок если>", "<тело>"],
        "def": ["<блок функции>"],
        ";": [";", "<тело>"],
        "for": ["for", "(", "mutable", "<действие>", "<действие>", "<выражение>", ")", "{", "<тело>", "}"],
        "mutable" : ["<модификатор доступа>", "<идентификатор>", ":", "<тип>", ";", "<тело>"],
        "}": [],
        "else": [],
        "return" : ["<return>"],
        

        "<<dont_push_if_empty>>" : true,
        "<<dont_recursion_reversal>>":true
    },
    "<return>": {
        "return" :["return", "<действие>"]
    },

    "<обявы через запятую>" : {
        "," : ["<запятая>", "<идентификатор>", "<обявы через запятую>"],
        ";" : []
    },
    "<идентификатор>" : { 
        "id":["<value_id>"],
        ";":[],
        "<<dont_push_if_empty>>" : true
    },

    "<блок если>": {
        "if" : ["if", "(", "<предусловие>", ")", "{", "<тело>", "<блок иначе>", "}"]
    },
    "<модификатор доступа>": {
        "mutable":["mutable"],
        "def":["def"]
    },
    "<блок иначе>": {
        "else":["<else>", "<тело>"],
        "}":[]
    },
    "<else>" : {"else" : ["else"]},

    "<предусловие>" : {
        "id" : ["<выражение>"],
        "num" : ["<выражение>"]
    },
    "<действие>": { 
        "id" : ["<выражение>", ";"],
        "num": ["<выражение>", ";"]  
    }, 
    "<аргументы вызываемой функции>": { 
        ")": [], 
        "id": ["<выражение>", "<аргументы вызываемой функции с запятой>"],
        "num" : ["<выражение>", "<аргументы вызываемой функции с запятой>"]
    },
    "<аргументы вызываемой функции с запятой>": {
        "," : ["<запятая>", "<выражение>", "<аргументы вызываемой функции с запятой>"],
        ")" : []
    },
    "<блок цикла>": {

    },
    "<пр. выр.>": { 
        "+": ["<последовательность слагаемых>"], 
        "-": ["<последовательность слагаемых>"], 
        "(": ["(","<пр. выр.>",")"], 
        "id": ["<слагаемое>", "<последовательность слагаемых>"],
        "num" : ["<слагаемое>", "<последовательность слагаемых>"] 
    },
    "<выражение>" : {
        "(": ["<откр. скобка>","<выражение>","<закр. скобка>", "<часть выражения>"],
        "id": ["<value_id>", "<аргументы для id>", "<часть выражения>"],
        "num": ["<value_num>", "<часть выражения>"],
        "-" : ["<выражение>"],
        "+" : ["<выражение>"],
        "<<EMPTY PRODUCTIONS>>" : [")"]
    },
    "<часть выражения>": {
        "id" : ["<выражение>"],
        "num": ["<выражение>"],
        ">" : ["<больше>",  "<выражение>", "<часть выражения>"],
        "<" : ["<меньше>",  "<выражение>", "<часть выражения>"],
        "=":["<присвоить>",   "<выражение>", "<часть выражения>"], 
        ">=":["<больше-равно>", "<выражение>", "<часть выражения>"], 
        "==":["<равно>", "<выражение>", "<часть выражения>"], 
        "<=":["<меньше-равно>", "<выражение>", "<часть выражения>"],
        "+=":["<прибавить и происвоить>", "<выражение>", "<часть выражения>"],  
        "!=":["<неравно>", "<выражение>", "<часть выражения>"],
        "+" : ["<плюс>",  "<выражение>", "<часть выражения>"],
        "-" : ["<минус>",  "<выражение>", "<часть выражения>"],
        "*" : ["<умножить>",  "<выражение>", "<часть выражения>"],
        "/" : ["<делить>",  "<выражение>", "<часть выражения>"],
        "and" : ["<and>",  "<выражение>", "<часть выражения>"],
        "or" : ["<or>",  "<выражение>", "<часть выражения>"],
        ":" : ["<двоеточие>",  "<выражение>", "<часть выражения>"],

        "<<EMPTY PRODUCTIONS>>": [";", ")", "{", "}", ","]
    },
    "<аргументы для id>": {
        "(": ["(","<аргументы вызываемой функции>",")"],
        "<<EMPTY PRODUCTIONS>>": ["+", "-", ":",">","+=", "=","==", "<", ">=", "!=", ";", "<=", "/", "+=", "and", "or", "*", "/", ")"]
    }
    
}